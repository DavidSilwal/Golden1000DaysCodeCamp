@model IEnumerable<WebApplication.Data.MotherExamination>

@{
    ViewData["Title"] = "Index";
  
}


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Advice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BloodPressure)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Complication)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HBTest)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Height)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Medication)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Weight)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Advice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BloodPressure)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Complication)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HBTest)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Height)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Medication)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Weight)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ExaminationID" type="button" class="btn btn-info">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ExaminationID" type="button" class="btn btn-info">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ExaminationID" type="button" class="btn btn-info">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
